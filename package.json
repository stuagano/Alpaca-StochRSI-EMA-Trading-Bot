{
  "name": "alpaca-stochrsi-ema-trading-bot",
  "version": "1.0.0",
  "description": "Trading Bot built using the Alpaca API in Python. Indicators used for Signal Generation: EMA, StochRSI, and Stochastic Oscillator",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project='mobile-*'",
    "test:api": "playwright test tests/e2e/api",
    "test:dashboard": "playwright test tests/e2e/dashboard",
    "test:portfolio": "playwright test tests/e2e/portfolio",
    "test:trading": "playwright test tests/e2e/trading",
    "test:report": "playwright show-report",
    "test:codegen": "playwright codegen http://localhost:5000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tejaslinge/Alpaca-StochRSI-EMA-Trading-Bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/tejaslinge/Alpaca-StochRSI-EMA-Trading-Bot/issues"
  },
  "homepage": "https://github.com/tejaslinge/Alpaca-StochRSI-EMA-Trading-Bot#readme",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "playwright": "^1.55.0"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
